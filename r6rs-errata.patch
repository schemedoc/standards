+(define get-csv
+  ;csv == comma-separated value
+  (lambda ()
+    (ignorespaces)
+    (let ((rev-lbl
+            (let loop ((s '()) (nesting 0))
+              (let ((c (get-actual-char)))
+                (cond ((eof-object? c)
+                       (terror 'get-csv
+                         "Runaway argument of \\cite, "
+                         "\\nocite, \\expandhtmlindex?")
+                       s)
