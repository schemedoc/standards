+(define ungroup
+  (lambda (s)
+    (let* ((n (string-length s))
+           (n-1 (- n 1)))
+      (if (or (< n 2)
+              (not (char=? (string-ref s 0) #\{))
+              (not (char=? (string-ref s n-1) #\})))
+          s
+          (substring s 1 n-1)))))
