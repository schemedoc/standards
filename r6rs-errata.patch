+(define if-aware-ctl-seq?
+  (lambda (z)
+    (or (ormap (lambda (y) (string=? z y))
+                 *if-aware-ctl-seqs*)
+        (and (>= (string-length z) 3)
+             (char=? (string-ref z 1) #\i)
+             (char=? (string-ref z 2) #\f))
+       (let ((z-th (find-corresp-prim-thunk z)))
+          (if (string? z-th) #f
+              (ormap
+               (lambda (y)
+                 (eq? z-th (find-corresp-prim-thunk y)))
+                 *if-aware-ctl-seqs*))))))
