+      (let ((n (string->number
+                (list->string
+                 (nreverse
+                  (let loop ((s '()))
+                    (let ((c (snoop-actual-char)))
+                      (cond ((eof-object? c) s)
+                            ((or (char-numeric? c) (char=? c #\.))
+                             (get-actual-char)
+                             (loop (cons c s)))
