+             => (lambda (key)
+                  (write-bib-aux "\\citation{")
+                  (write-bib-aux key)
+                  (write-bib-aux "}")
+                  (write-bib-aux #\newline)
+                  (loop)))))
+    (unless (char=? (get-actual-char) #\})
+      (terror 'do-nocite "Missing }"))
+    ))
