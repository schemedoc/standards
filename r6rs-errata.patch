+(define get-ctl-seq
+  (lambda ()
+    ;read a tex \controlsequence
+    ;(ignorespaces)
+    (let ((bs (get-actual-char)))
+      (unless (char=? bs *esc-char*)
+        (terror 'get-ctl-seq "Missing control sequence (" bs ")")))
+    (let ((c (get-char)))
+      (cond ((eof-object? c) "\\ ")
+            ((invisible-space? c) "\\ ")
+            ((char-tex-alphabetic? c)
+             (list->string
+              (nreverse
+               (let loop ((s (list c #\\ )))
+                 (let ((c (snoop-char)))
+                   (cond ((eof-object? c) s)
+                         ((invisible-space? c) s)
+                         ((char-tex-alphabetic? c) (get-char)
+                          (loop (cons c s)))
+                         (else
+                          (unless (or ;*in-verb?*
+                                      *math-mode?*
+                                      *not-processing?*
+                                      (eq? *tex-format* ':texinfo)
+                                      )
+                            (fluid-let ((*reading-control-sequence?* #t))
+                              (ignorespaces))
+                            )
+                          s)))))))
+            (else (string #\\ c))))))
