+(define file-stem-name
+  (lambda (f)
+    ;chop off f's dirname and extension
+    (let ((slash (string-reverse-index f #\/)))
+      (when slash
+        (set! f (substring f (+ slash 1) (string-length f))))
+      (let ((dot (string-reverse-index f #\.)))
+        (if dot
+            (substring f 0 dot)
+            f)))))
