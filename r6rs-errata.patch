+      (emit "<span style=\"margin-left: ")
+      (emit parindent)
+      (emit "pt\">&zwnj;</span>")
+      (emit "<span style=\"position: relative\">&zwnj;")
+      (emit "<span style=\"position: absolute; left: -")
+      (emit parindent)
+      (emit "pt\">")
+      (tex2page-string (get-group))
+      (ignorespaces)
+      (emit-nbsp 2)
+      (emit "</span></span>"))))
