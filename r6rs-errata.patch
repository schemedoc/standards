+(define table-put!
+  (lambda (tbl k v)
+    (let ((al (table.alist tbl)))
+      (let ((c (lassoc k al (table.equ tbl))))
+        (if c (vector-set! (cdr c) 0 v)
+            (set!table.alist tbl (cons (cons k (vector v)) al)))))))
