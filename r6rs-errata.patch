+(define emit-html-char
+  (lambda (c)
+    ;display the html equiv of char c
+    (unless (eof-object? c)
+      (cond ((char=? c #\newline) (emit-newline))
+            (*outputting-to-non-html?* (emit c))
+            (else
+             (case c
+               ((#\<) (emit "&lt;"))
+               ((#\>) (emit "&gt;"))
+               ((#\") (emit "&quot;"))
+               ((#\&) (emit "&amp;"))
+               (else (emit c))))))))
