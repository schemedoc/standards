+               (emit
+               (if *verb-visible-space?*
+                   *verbatim-visible-space*
+ #\space))
+               (loop))
+              ((char=? c #\newline)
+               (cond (*verb-display?* (emit "&nbsp;")
+                                      (emit-newline))
+                     (*verb-visible-space?*
+                      (emit *verbatim-visible-space*))
