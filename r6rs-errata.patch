+              (begin
+               (case c
+                 ((#\\ ) (do-tex-ctl-seq (get-ctl-seq)))
+                 ((#\{) (get-actual-char) (bgroup))
+                 ((#\}) (get-actual-char) (egroup))
+                 (else (emit-html-char (get-actual-char))))
