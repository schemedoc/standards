+(define do-makehtmlimage
+  (lambda ()
+    (ignorespaces)
+    (unless (char=? (snoop-actual-char) #\{)
+      (terror 'do-makehtmlimage "\\makehtmlimage's argument must be a group"))
+     (call-with-html-image-port dump-groupoid)))
