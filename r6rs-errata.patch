+(define table-get
+  (lambda (tbl k . d)
+    (cond ((lassoc k (table.alist tbl) (table.equ tbl))
+           => (lambda (c) (vector-ref (cdr c) 0)))
+          ((pair? d) (car d))
+          (else #f))))
