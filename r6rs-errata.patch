+(define file-extension
+  (lambda (f)
+    ;returns filename f's extension, if any
+    (let ((slash (string-reverse-index f #\/))
+          (dot (string-reverse-index f #\.)))
+      (if (and dot (not (= dot 0))
+               (or (not slash) (< (+ slash 1) dot)))
+          (substring f dot (string-length f))
+          #f))))
