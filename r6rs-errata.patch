+(define file-in-home
+  (lambda (f)
+    (let ((home (getenv "HOME")))
+      (and home
+           (let ((slash-already?
+                  (let ((n (string-length home)))
+                    (and (>= n 0)
+                         (let ((c (string-ref home (- n 1))))
+                           (or (char=? c #\/)
+                               (char=? c #\\ )))))))
+             (string-append home
+                            (if slash-already? "" "/")
+                            f))))))
