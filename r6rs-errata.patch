+(define munch-newlines
+  (lambda ()
+    (let loop ((n 0))
+      (let ((c (snoop-actual-char)))
+        (cond ((eof-object? c) n)
+              ((char=? c #\newline) (get-actual-char) (loop (+ n 1)))
+              ((char-whitespace? c) (get-actual-char) (loop n))
+              (else n))))))
+
+(define munched-a-newline?
+  (lambda ()
+    (let loop ()
+      (let ((c (snoop-actual-char)))
+        (cond ((eof-object? c) #f)
+              ((char=? c #\newline)
+               (get-actual-char) #t)
+              ((char-whitespace? c)
+               (get-actual-char) (loop))
+              (else #f))))))
