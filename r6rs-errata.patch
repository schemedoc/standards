+(define path-to-list
+  (lambda (p)
+    ;convert an OS path into a Scheme list
+    (if (not p) '()
+        (let loop ((p p) (r '()))
+          (let ((i (string-index p *path-separator*)))
+            (if i
+                (loop (substring p (+ i 1) (string-length p))
+                  (cons (substring p 0 i) r))
+                (nreverse (cons p r))))))))
