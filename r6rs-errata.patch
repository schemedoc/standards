+(define do-item
+  (lambda ()
+    (let ((a #f))
+      (unless (null? *tabular-stack*)
+        (set! a (car *tabular-stack*)))
+      (case a
+        ((:description) (do-description-item))
+        ((:itemize :enumerate) (do-regular-item))
+        (else (do-plain-item 1))))))
