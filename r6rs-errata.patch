+(define do-plain-item
+  (lambda (n)
+    (let ((parindent (sp-to-pixels (find-dimen "\\parindent"))))
+      (do-end-para)
+      (emit "<table><tr><td width=")
+      (emit parindent)
+      (emit " valign=top align=right>")
+      (let loop ((n n))
+        (unless (<= n 1)
+          (emit "</td><td width=")
+          (emit parindent)
+          (emit " valign=top align=right>")
+          (loop (- n 1))))
+      (tex2page-string (get-group))
+      (emit-nbsp 2)
