+(define edit-offending-file
+  (lambda ()
+    (let ((bad-texedit? #f)
+          (cmd #f))
+      (cond ((getenv "TEXEDIT")
+             => (lambda (s)
+                  (cond ((substring? "%d" s)
+                         => (lambda (i)
+                              (set! s
+                                (string-append (substring s 0 i)
+                                               (number->string *input-line-no*)
+                                               (substring s (+ i 2)
+                                                          (string-length s))))))
+                        (else (set! bad-texedit? #t)))
+                  (cond ((and (not bad-texedit?)
+                              (substring? "%s" s))
+                         => (lambda (i)
+                              (set! s
+                                (string-append (substring s 0 i)
+                                               *current-source-file*
+                                               (substring s (+ i 2)
+                                                          (string-length s))))))
+                        (else (set! bad-texedit? #t)))
+                  (cond (bad-texedit?
+                          (display "Bad TEXEDIT; using EDITOR.") (newline))
+                        (else (set! cmd s))))))
+      (cond ((and (not cmd) (or (getenv "EDITOR") "vi"))
+             => (lambda (s)
+                  (set! cmd
+                    (string-append
+                      s " +" (number->string *input-line-no*)
+                      " " *current-source-file*)))))
+      (when cmd
+        (system cmd)))))
